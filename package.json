{
  "name": "PrismaRedPill",
  "version": "1.0.0",
  "description": "Backend Redpill",
  "author": "Nevercomex",
  "bin": "dist/src/server.js",
  "private": true,
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "nodemon --files src/server.ts",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "start": "node ./dist/src/server.js",
    "test": "jest --runInBand",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ext .ts .",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "initialize": "yarn && ts-node bin/commands/initialize.ts && prettier --write package.json",
    "make": "ts-node bin/commands/make.ts",
    "clean:empties": "find src -type d -empty -delete",
    "clean": "rimraf dist && npm run clean:empties"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/prisma": "^1.4.0",
    "@prisma/client": "^4.16.2",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "bcrypt": "^5.1.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mailgun": "^0.5.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "husky": "^8.0.3",
    "inquirer": "^9.2.7",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "prisma": "^4.16.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
